<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Admin Page</title>
</head>
<body>
    <h1>Admin Page - Guestbook Entries</h1>

    <table border="1">
        <thead>
            <tr>
                <th>ID</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Job Title</th>
                <th>Company</th>
                <th>LinkedIn</th>
                <th>Email</th>
                <th>Message</th>
                <th>Date Submitted</th>
            </tr>
        </thead>
        <tbody>
            <% entries.forEach(form => { %>
                <tr>
                    <td><%= form.id %></td>
                    <td><%= form.firstname %></td>
                    <td><%= form.lastname %></td>
                    <td><%= form.jobname %></td>
                    <td><%= form.company %></td>
                    <td><%= form.linkedin %></td>
                    <td><%= form.email %></td>
                    <td><%= form.message %></td>
                    <td><%= form.created_at %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>

</body>
</html>
